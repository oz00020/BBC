
<div class="tile">

<h1> Ranking Articles </h1>
<br>

<ul><li><h4><%= link_to "Rank Article 1", new_page_rank_path(@p1) %></h4><%= render @p1.ranks %></li></ul>
<ul><li><h4><%= link_to "Rank Article 2", new_page_rank_path(@p2) %></h4><%= render @p2.ranks %></li></ul>
<ul><li><h4><%= link_to "Rank Article 3", new_page_rank_path(@p3) %></h4><%= render @p3.ranks %></li></ul>
<ul><li><h4><%= link_to "Rank Article 4", new_page_rank_path(@p4) %></h4><%= render @p4.ranks %></li></ul>
